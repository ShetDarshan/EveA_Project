    "        #print(Price)\n",
    "        #print(title)\n",
    "        data = {}\n",
    "        data[\"title\"] = title\n",
    "        data[\"url\"]=read_more\n",
    "        data[\"price\"] = Price\n",
    "        data[\"image\"] = image\n",
    "        data[\"date\"] = Date\n",
    "        data[\"time\"] =''\n",
    "        data[\"location\"] = Location\n",
    "        data[\"category\"] = category\n",
    "        data[\"description\"] = str1\n",
    "    \n",
